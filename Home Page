    <script>
        $(document).ready(function() {
            let currentSlide = 0;
            const slides = $('.testimonial-slide');
            const totalSlides = slides.length;

            function goToSlide(index) {
                slides.removeClass('active-slide');
                $(slides[index]).addClass('active-slide');
                currentSlide = index;
            }

            $('.testimonial-next').on('click', function(e) {
                e.preventDefault();
                let nextSlide = (currentSlide + 1) % totalSlides;
                goToSlide(nextSlide);
            });

            $('.testimonial-prev').on('click', function(e) {
                e.preventDefault();
                let prevSlide = (currentSlide - 1 + totalSlides) % totalSlides;
                goToSlide(prevSlide);
            });
        });
    </script>
